{
  "name": "protempo",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,json}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "e2e:build:ios": "detox build --configuration ios.sim.release",
    "e2e:build:android": "detox build --configuration android.emu.release",
    "e2e:test:ios": "detox test --configuration ios.sim.release",
    "e2e:test:android": "detox test --configuration android.emu.release",
    "e2e:ios": "npm run e2e:build:ios && npm run e2e:test:ios",
    "e2e:android": "npm run e2e:build:android && npm run e2e:test:android",
    "e2e:build:ios:debug": "detox build --configuration ios.sim.debug",
    "e2e:test:ios:debug": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "^5.0.1",
    "expo": "~54.0.31",
    "expo-audio": "~1.1.1",
    "expo-av": "~16.0.8",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-haptics": "~15.0.8",
    "expo-keep-awake": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-video-thumbnails": "~10.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/detox": "^17.14.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "detox": "^20.46.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-circus": "^30.2.0",
    "jest-expo": "~54.0.0",
    "prettier": "^3.4.0",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
